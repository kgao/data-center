<!-- views/pages/index.ejs -->

<!doctype html>
<html lang="en">
<% include ../includes/head %>

  <body>
    <% include ../includes/nav %>
  
  <% if(type === 'iq'){   %>
      <% if(data){   %>
        <h2>DATA </h2>
        <h4>Total
          <%= data.length %> Documents</h4>
        <div class="table-responsive">
          <table id="data" class="table table-bordered table-hover table-striped">
            <thead>
              <tr>
                <th>id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Score</th>
                <th>IQ</th>
                <th>Time</th>
                <th>Date</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i<data.length; i++) { %>
                <tr>
                  <td>
                    <%= data[i].id %>
                  </td>
                  <td>
                    <%= data[i].name %>
                  </td>
                  <td>
                    <%= data[i].email %>
                  </td>
                  <td>
                    <%= data[i].score %>
                  </td>
                  <td>
                    <%= data[i].IQ %>
                  </td>
                  <td>
                    <%= data[i].time %>
                  </td>
                  <td>
                    <%= data[i].createdAt %>
                  </td>
                </tr>
                <% } %>
            </tbody>
          </table>
        </div>

        <% } %>
 <% } %>

  <% if(type === 'gemcrush'){   %>


        
      <% if(data){   %>
        <h2>DATA </h2>
        <h4>Total
          <%= data.length %> Documents</h4>
        <div class="table-responsive">
          <table id="data" class="table table-bordered table-hover table-striped">
            <thead>
              <tr>
                <th>id</th>
                <th>Name</th>
                <th>Score</th>
                <th>Date</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i=0; i<data.length; i++) { %>
                <tr>
                  <td>
                    <%= data[i].id %>
                  </td>
                  <td>
                    <%= data[i].name %>
                  </td>
                  <td>
                    <%= data[i].score %>
                  </td>
                  <td>
                    <%= data[i].createdAt %>
                  </td>
                </tr>
                <% } %>
            </tbody>
          </table>
        </div>

        <% } %>
   <% } %>









  </body>

</html>